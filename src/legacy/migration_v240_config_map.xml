<?xml version="1.0" encoding="UTF-8" ?>
<default>
    <os>
        <version>UBUNTU_VERSION</version>
    </os>
    <nginx>
        <port>
            <unsecure>NGINX_UNSECURE_PORT</unsecure>
            <secure>NGINX_SECURE_PORT</secure>
            <internal>NGINX_INTERNAL_PORT</internal>
        </port>
        <ssl>
            <enabled>SSL</enabled>
        </ssl>
        <rut_type>MAGENTO_RUN_TYPE</rut_type>
        <default_host_first_level>DEFAULT_HOST_FIRST_LEVEL</default_host_first_level>
        <interface_ip>INTERFACE_IP</interface_ip>
    </nginx>
    <platform>PLATFORM</platform>
    <workdir>WORKDIR</workdir>
    <public_dir>PUBLIC_DIR</public_dir>
    <php>
        <version>PHP_VERSION</version>
        <composer>
            <version>PHP_COMPOSER_VERSION</version>
        </composer>
        <timezone>PHP_TZ</timezone>
        <xdebug>
            <version>XDEBUG_VERSION</version>
            <XDEBUG_IDE_KEY>PHPSTORM</XDEBUG_IDE_KEY>
            <remote_host>XDEBUG_REMOTE_HOST</remote_host>
            <enabled>XDEBUG_ENABLED</enabled>
            <mode>XDEBUG_MODE</mode>
        </xdebug>
        <ioncube>
            <enabled>IONCUBE_ENABLED</enabled>
        </ioncube>
    </php>
    <db>
        <repository>DB_REPOSITORY</repository>
        <root_password>DB_ROOT_PASSWORD</root_password>
        <user>DB_USER</user>
        <password>DB_PASSWORD</password>
        <database>DB_DATABASE</database>
        <phpmyadmin>
            <enabled>PHPMYADMIN_ENABLED</enabled>
            <repository>PHPMYADMIN2_REPOSITORY</repository>
            <version>PHPMYADMIN_VERSION</version>
        </phpmyadmin>
    </db>
    <db2>
        <enabled>DB2_ENABLED</enabled>
        <repository>DB2_REPOSITORY</repository>
        <root_password>DB2_ROOT_PASSWORD</root_password>
        <user>DB2_USER</user>
        <password>DB2_PASSWORD</password>
        <database>DB2_DATABASE</database>
        <phpmyadmin>
            <enabled>PHPMYADMIN2_ENABLED</enabled>
            <repository>PHPMYADMIN2_REPOSITORY</repository>
            <version>PHPMYADMIN2_VERSION</version>
        </phpmyadmin>
    </db2>
    <nodejs>
        <enabled>NODEJS_ENABLED</enabled>
        <version>NODEJS_VERSION</version>
        <yarn>
            <enabled>YARN_ENABLED</enabled>
            <version>YARN_VERSION</version>
        </yarn>
    </nodejs>
    <search>
        <engine>SEARCH_ENGINE</engine>
        <elasticsearch>
            <enabled>ELASTICSEARCH_ENABLED</enabled>
            <repository>ELASTICSEARCH_REPOSITORY</repository>
            <version>ELASTICSEARCH_VERSION</version>
            <dashboard>
                <enabled>KIBANA_ENABLED</enabled>
                <repository>KIBANA_REPOSITORY</repository>
            </dashboard>
        </elasticsearch>
        <opensearch>
            <enabled>OPENSEARCH_ENABLED</enabled>
            <repository>OPENSEARCH_REPOSITORY</repository>
            <version>OPENSEARCH_VERSION</version>
            <dashboard>
                <enabled>OPENSEARCHDASHBOARD_ENABLED</enabled>
                <repository>OPENSEARCHDASHBOARD_REPOSITORY</repository>
            </dashboard>
        </opensearch>
    </search>
    <redis>
        <enabled>REDIS_ENABLED</enabled>
        <repository>REDIS_REPOSITORY</repository>
        <version>REDIS_VERSION</version>
    </redis>
    <rabbitmq>
        <enabled>RABBITMQ_ENABLED</enabled>
        <repository>RABBITMQ_REPOSITORY</repository>
        <version>RABBITMQ_VERSION</version>
    </rabbitmq>
    <cron>
        <enabled>CRON_ENABLED</enabled>
    </cron>
    <ssh>
        <auth_type>SSH_AUTH_TYPE</auth_type>
        <host>SSH_HOST</host>
        <port>SSH_PORT</port>
        <username>SSH_USERNAME</username>
        <key_path>SSH_KEY_PATH</key_path>
        <password>SSH_PASSWORD</password>
        <site_root_path>SSH_SITE_ROOT_PATH</site_root_path>
    </ssh>
    <magento>
        <admin_first_name>MAGENTO_ADMIN_FIRST_NAME</admin_first_name>
        <admin_last_name>MAGENTO_ADMIN_LAST_NAME</admin_last_name>
        <admin_email>MAGENTO_ADMIN_EMAIL</admin_email>
        <admin_user>MAGENTO_ADMIN_USER</admin_user>
        <admin_password>MAGENTO_ADMIN_PASSWORD</admin_password>
        <admin_frontname>MAGENTO_ADMIN_FRONTNAME</admin_frontname>
        <locale>MAGENTO_LOCALE</locale>
        <currency>MAGENTO_CURRENCY</currency>
        <timezone>MAGENTO_TIMEZONE</timezone>
        <mftf>
            <enabled>MFTF_ENABLED</enabled>
            <admin_user>MFTF_ADMIN_USER</admin_user>
            <otp_shared_secret>MFTF_OTP_SHARED_SECRET</otp_shared_secret>
        </mftf>
        <cloud>
            <enabled>MAGENTOCLOUD_ENABLED</enabled>
            <username>MAGENTOCLOUD_USERNAME</username>
            <password>MAGENTOCLOUD_PASSWORD</password>
            <project_name>MAGENTOCLOUD_PROJECT_NAME</project_name>
        </cloud>
        <n98magerun>
            <enabled>N98MAGERUN_ENABLED</enabled>
        </n98magerun>
    </magento>
    <proxy>
        <enabled>PROXY_ENABLED</enabled>
    </proxy>
    <pwa>
        <backend_url>PWA_BACKEND_URL</backend_url>
    </pwa>
    <container_name_prefix>CONTAINER_NAME_PREFIX</container_name_prefix>
    <cache_hash>CACHE_HASH</cache_hash>
    <path>PATH</path>
</default>
